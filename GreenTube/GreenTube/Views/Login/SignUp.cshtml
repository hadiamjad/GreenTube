
@{
    ViewBag.Title = "SignUp";
}

<h2>This is the sign-up page.</h2>

<div class="jumbotron mbox" id="blackBox">
    <form class="mform" _lpchecked="1" runat="server" action="../Login/AddUser" method="post">
        <fieldset>

            <div class="form-group">
                <label for="_name">Name</label>
                <input required type="text" class="form-control" name="_name" id="_name" placeholder="First Name Last Name" style="background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; cursor: pointer;">
            </div>


            <div class="form-group">
                <label for="userId">Email address</label>
                <input required type="text" class="form-control" name="userId" id="userId" aria-describedby="emailHelp" placeholder="Enter email" style="background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; cursor: auto;">
                <small id="emailComment" class="form-text text-muted">enter your email here.</small>
            </div>

            <div class="form-group">
                <label for="userName">Username</label>
                <input required type="text" class="form-control" name="userName" id="userName" aria-describedby="emailHelp" placeholder="Your Personalized Name" style="background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; cursor: auto;">
                <small id="emailComment" class="form-text text-muted">The username should be unique!</small>
            </div>


            <div class="form-group">
                <label for="_password">Password</label>
                <input required type="password" class="form-control" name="_password" id="_password" placeholder="Password" style="background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; cursor: pointer;">
            </div>

            <div class="form-group">
                <label for="_password2">Re-Enter Password</label>
                <input required type="password" class="form-control" name="_password" id="_password2" placeholder="Password" style="background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; cursor: pointer;">
            </div>

            <div class="form-group">
                <label for="_date">Date of Birth</label>
                <input required type="date" class="form-control" name="_date" id="_date" />
                <small id="emailComment" class="form-text text-muted">Juvenile audience cannot be creators.</small>
            </div>


            <button type="submit" class="btn btn-primary btn-danger btn-lg btn-block" id="_submit">SUBMIT</button>
        </fieldset>
    </form>
</div>

